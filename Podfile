# platform :ios, '9.0'

target 'RxSwiftDemo' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
	pod 'RxSwift',    '~> 2.0'
    	pod 'RxCocoa',    '~> 2.0'
  # Pods for RxSwiftDemo

  target 'RxSwiftDemoTests' do
    inherit! :search_paths
    # Pods for testing
	pod 'RxBlocking', '~> 2.0'
    	pod 'RxTests',    '~> 2.0'
  end

end
